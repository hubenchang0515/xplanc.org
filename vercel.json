{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/primers/:path*",
      "destination": "https://primers.xplanc.org/primers/:path*"
    },

    {
      "source": "/shift/:path*",
      "destination": "https://shift.xplanc.org/shift/:path*"
    },

    {
      "source": "/QtTheme/:path*",
      "destination": "https://qttheme.xplanc.org/QtTheme/:path*"
    }
  ],

  "redirects": [
    {
      "source": "/primers",
      "destination": "/primers/",
      "permanent": true
    },

    {
      "source": "/shift",
      "destination": "/shift/",
      "permanent": true
    },

    {
      "source": "/QtTheme",
      "destination": "/QtTheme/",
      "permanent": true
    }
  ]
}
