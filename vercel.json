{
  "cleanUrls": true,
  "rewrites": [
    
    {
      "source": "/primers/",
      "destination": "https://primers.xplanc.org/primers/"
    },

    {
      "source": "/primers/:path*",
      "destination": "https://primers.xplanc.org/primers/:path*"
    },

    {
      "source": "/shift/",
      "destination": "https://shift.xplanc.org/shift/"
    },

    {
      "source": "/shift/:path*",
      "destination": "https://shift.xplanc.org/shift/:path*"
    },

    {
      "source": "/QtTheme/",
      "destination": "https://qttheme.xplanc.org/QtTheme/"
    },

    {
      "source": "/QtTheme/:path*",
      "destination": "https://qttheme.xplanc.org/QtTheme/:path*"
    },

    {
      "source": "/Phosphophyllite/",
      "destination": "https://blog.xplanc.org/Phosphophyllite/"
    },

    {
      "source": "/Phosphophyllite/:path*",
      "destination": "https://blog.xplanc.org/Phosphophyllite/:path*"
    }
  ],

  "redirects": [
    {
      "source": "/primers",
      "destination": "/primers/",
      "permanent": true
    },

    {
      "source": "/shift",
      "destination": "/shift/",
      "permanent": true
    },

    {
      "source": "/QtTheme",
      "destination": "/QtTheme/",
      "permanent": true
    },

    {
      "source": "/Phosphophyllite",
      "destination": "/Phosphophyllite/",
      "permanent": true
    }
  ]
}
