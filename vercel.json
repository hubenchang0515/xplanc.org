{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/primers/",
      "destination": "https://primers.xplanc.org/"
    },

    {
      "source": "/primers/:path*",
      "destination": "https://primers.xplanc.org/:path*"
    },

    {
      "source": "/shift/",
      "destination": "https://shift.xplanc.org/"
    },

    {
      "source": "/shift/:path*",
      "destination": "https://shift.xplanc.org/:path*"
    },

    {
      "source": "/Phosphophyllite/",
      "destination": "https://blog.xplanc.org/"
    },

    {
      "source": "/Phosphophyllite/:path*",
      "destination": "https://blog.xplanc.org/:path*"
    }
  ],

  "redirects": [
    {
      "source": "/primers",
      "destination": "/primers/",
      "permanent": true
    },

    {
      "source": "/primers/",
      "destination": "/primers/document/zh",
      "permanent": false
    },

    {
      "source": "/shift",
      "destination": "/shift/",
      "permanent": true
    },

    {
      "source": "/uptime",
      "destination": "https://stats.uptimerobot.com/wTSqLfXezb",
      "permanent": true
    },

    {
      "source": "/QtTheme/",
      "destination": "https://hubenchang0515.github.io/QtTheme/",
      "permanent": true
    },

    {
      "source": "/QtTheme/:path*",
      "destination": "https://hubenchang0515.github.io/QtTheme/:path*",
      "permanent": true
    },

    {
      "source": "/Phosphophyllite",
      "destination": "/Phosphophyllite/",
      "permanent": true
    }
  ]
}
