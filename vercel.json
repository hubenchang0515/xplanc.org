{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/primers/:path*",
      "destination": "https://primers.xplanc.org/:path*"
    },

    {
      "source": "/shift/:path*",
      "destination": "https://shift.xplanc.org/:path*"
    },

    {
      "source": "/QtTheme/:path*",
      "destination": "https://qttheme.xplanc.org/:path*"
    },

    {
      "source": "/:path*",
      "destination": "https://primers.xplanc.org/:path*"
    }
  ]
}
